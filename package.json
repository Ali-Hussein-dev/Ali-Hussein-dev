    "framer-motion": "^6.5.1",
    "next": "^12.2.5",
    "next-connect": "^0.12.2",
    "nextjs-progressbar": "^0.0.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.4.0",
    "video-react": "^0.15.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/react": "^17.0.1",
    "@types/video-react": "^0.15.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "autoprefixer": "^10.4.4",
