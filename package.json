    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "postcss": "^8.4.12",
    "prettier": "^2.0.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ali-Hussein-dev/Ali-Hussein-dev"
  }
